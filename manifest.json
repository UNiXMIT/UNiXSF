{
  "name": "Salesforce Extension",
  "version": "2.1",
  "description": "Enhance Salesforce with new features.",
  "author": "Martin Turner",
  "homepage_url": "https://unixmit.github.io/UNiXSF",
  "manifest_version": 3,
  "options_page": "config.html",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.lightning.force.com/*"
      ],
      "run_at": "document_end",
      "js": ["SFExt.js"]
    }
  ],
  "icons": {
    "16": "icons/mf16.png",
    "32": "icons/mf32.png",
    "48": "icons/mf48.png",
    "128": "icons/mf128.png"
  }
}